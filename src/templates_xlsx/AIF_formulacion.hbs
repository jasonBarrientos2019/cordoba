<table>

    <tr  text-align="center">
        <td id="img"><img src="../assets/images/gobierno.jpg" alt=""> </td>
        <td colspan="2">Cuenta Ahorro - Inversión - Financiamiento Presupuesto </td>
        <td background="002642" color="ffffff">Fecha de Confección</td>
    </tr>

    <tr text-align="center">
        <td> </td>
        <td colspan="2">{{presupuesto}}</td>
        <td >{{date "now" "dd/MM/yyyy"}} {{date "now" "hh:mm"}}</td>
    </tr>

    <tr>
        {{!-- vacia --}}
    </tr>

    <tr background="002642" color="ffffff">
        <td> </td>
        <td>Anualizado</td>
        <td>Anteproyecto</td>
        <td>Proyecto Ley</td>
    </tr>

    {{#each montos}}
    {{#each groups}}
    <tr background="2B658F" color="ffffff">
        <td> {{nombreGroup}} </td>
        <td> {{montoAnualizado}}</td>
        <td> {{montoAnteproyecto}}</td>
        <td> {{proyectoLey}}</td>
    </tr>
    {{#each elementosGroups}}
    <tr color="6187A2">
        <td> {{elementosGroup}} </td>
        <td> {{montoAnualizado}}</td>
        <td> {{montoAnteproyecto}}</td>
        <td> {{proyectoLey}}</td>
    </tr>
    {{#each elementos}}
    <tr text-align="right">
        <td> {{nombreElemento}} </td>
        <td> {{montoAnualizado}}</td>
        <td> {{montoAnteproyecto}}</td>
        <td> {{proyectoLey}}</td>
    </tr>

    {{/each}}
    {{/each}}
    {{/each}}

    <tr background="6187A2" color="ffffff">
        <td> {{montoName}} </td>
        <td> {{montoAnualizado}}</td>
        <td> {{montoAnteproyecto}}</td>
        <td> {{proyectoLey}}</td>
    </tr>

    {{/each}}


</table>